{
  "consumer": {
    "name": "access-checkout-iOS-sdk"
  },
  "provider": {
    "name": "sessions"
  },
  "interactions": [
    {
      "description": "GET request to /sessions to discover card session endpoint",
      "request": {
        "method": "get",
        "path": "/sessions",
        "headers": {
          "Accept": "application/vnd.worldpay.sessions-v1.hal+json",
          "content-type": "application/vnd.worldpay.sessions-v1.hal+json"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/vnd.worldpay.sessions-v1.hal+json"
        },
        "body": {
          "_links": {
            "sessions:paymentsCvc": {
              "href": "http://pacttest/sessions/payments/cvc"
            },
            "sessions:card": {
              "href": "http://pacttest/sessions/card"
            }
          }
        },
        "matchingRules": {
          "$.body._links.sessions:paymentsCvc.href": {
            "match": "regex",
            "regex": "https?:\\/\\/[^\\/]+\\/sessions\\/payments\\/cvc"
          },
          "$.body._links.sessions:card.href": {
            "match": "regex",
            "regex": "https?:\\/\\/[^\\/]+\\/sessions\\/card"
          }
        }
      }
    },
    {
      "description": "POST request to /sessions/card with PAN that does not pass Luhn check",
      "request": {
        "method": "post",
        "path": "/sessions/card",
        "headers": {
          "Accept": "application/vnd.worldpay.sessions-v1.hal+json",
          "content-type": "application/vnd.worldpay.sessions-v1.hal+json"
        },
        "body": {
          "cvc": "123",
          "cardExpiryDate": {
            "month": 12,
            "year": 2099
          },
          "identity": "identity",
          "cardNumber": "4111111111111110"
        }
      },
      "response": {
        "status": 400,
        "headers": {
          "Content-Type": "application/vnd.worldpay.sessions-v1.hal+json"
        },
        "body": {
          "validationErrors": [
            {
              "errorName": "panFailedLuhnCheck",
              "message": "The identified field contains a PAN that has failed the Luhn check.",
              "jsonPath": "$.cardNumber"
            }
          ],
          "errorName": "bodyDoesNotMatchSchema",
          "message": "The json body provided does not match the expected schema"
        },
        "matchingRules": {
          "$.body.validationErrors[0].message": {
            "match": "type"
          },
          "$.body.message": {
            "match": "type"
          }
        }
      }
    },
    {
      "description": "POST request to /sessions/card with PAN containing letters",
      "request": {
        "method": "post",
        "path": "/sessions/card",
        "headers": {
          "Accept": "application/vnd.worldpay.sessions-v1.hal+json",
          "content-type": "application/vnd.worldpay.sessions-v1.hal+json"
        },
        "body": {
          "cardNumber": "notACardNumber",
          "cardExpiryDate": {
            "month": 1,
            "year": 2099
          },
          "identity": "identity",
          "cvc": "123"
        }
      },
      "response": {
        "status": 400,
        "headers": {
          "Content-Type": "application/vnd.worldpay.sessions-v1.hal+json"
        },
        "body": {
          "errorName": "bodyDoesNotMatchSchema",
          "message": "The json body provided does not match the expected schema",
          "validationErrors": [
            {
              "message": "Card number must be numeric",
              "errorName": "fieldHasInvalidValue",
              "jsonPath": "$.cardNumber"
            }
          ]
        },
        "matchingRules": {
          "$.body.message": {
            "match": "type"
          },
          "$.body.validationErrors[0].message": {
            "match": "type"
          }
        }
      }
    },
    {
      "description": "POST request to /sessions/card with invalid identity in body",
      "request": {
        "method": "post",
        "path": "/sessions/card",
        "headers": {
          "Accept": "application/vnd.worldpay.sessions-v1.hal+json",
          "content-type": "application/vnd.worldpay.sessions-v1.hal+json"
        },
        "body": {
          "cvc": "123",
          "cardExpiryDate": {
            "month": 12,
            "year": 2099
          },
          "cardNumber": "4111111111111111",
          "identity": "incorrectValue"
        }
      },
      "response": {
        "status": 400,
        "headers": {
          "Content-Type": "application/vnd.worldpay.sessions-v1.hal+json"
        },
        "body": {
          "validationErrors": [
            {
              "errorName": "fieldHasInvalidValue",
              "message": "Identity is invalid",
              "jsonPath": "$.identity"
            }
          ],
          "errorName": "bodyDoesNotMatchSchema",
          "message": "The json body provided does not match the expected schema"
        },
        "matchingRules": {
          "$.body.validationErrors[0].message": {
            "match": "type"
          },
          "$.body.message": {
            "match": "type"
          }
        }
      }
    },
    {
      "description": "POST request to /sessions/card with expiry date month that is too large",
      "request": {
        "method": "post",
        "path": "/sessions/card",
        "headers": {
          "Accept": "application/vnd.worldpay.sessions-v1.hal+json",
          "content-type": "application/vnd.worldpay.sessions-v1.hal+json"
        },
        "body": {
          "cardExpiryDate": {
            "year": 2099,
            "month": 13
          },
          "cardNumber": "4111111111111111",
          "identity": "identity",
          "cvc": "123"
        }
      },
      "response": {
        "status": 400,
        "headers": {
          "Content-Type": "application/vnd.worldpay.sessions-v1.hal+json"
        },
        "body": {
          "message": "The json body provided does not match the expected schema",
          "errorName": "bodyDoesNotMatchSchema",
          "validationErrors": [
            {
              "errorName": "integerIsTooLarge",
              "jsonPath": "$.cardExpiryDate.month",
              "message": "Card expiry month is too large - must be between 1 & 12"
            }
          ]
        },
        "matchingRules": {
          "$.body.message": {
            "match": "type"
          },
          "$.body.validationErrors[0].message": {
            "match": "type"
          }
        }
      }
    },
    {
      "description": "GET request to /sessions to discover cvc session endpoint",
      "request": {
        "method": "get",
        "path": "/sessions",
        "headers": {
          "content-type": "application/vnd.worldpay.sessions-v1.hal+json",
          "Accept": "application/vnd.worldpay.sessions-v1.hal+json"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/vnd.worldpay.sessions-v1.hal+json"
        },
        "body": {
          "_links": {
            "sessions:paymentsCvc": {
              "href": "http://pacttest/sessions/payments/cvc"
            },
            "sessions:card": {
              "href": "http://pacttest/sessions/card"
            }
          }
        },
        "matchingRules": {
          "$.body._links.sessions:paymentsCvc.href": {
            "match": "regex",
            "regex": "https?:\\/\\/[^\\/]+\\/sessions\\/payments\\/cvc"
          },
          "$.body._links.sessions:card.href": {
            "match": "regex",
            "regex": "https?:\\/\\/[^\\/]+\\/sessions\\/card"
          }
        }
      }
    },
    {
      "description": "POST request to /sessions/payments/cvc with invalid identity in body",
      "request": {
        "method": "post",
        "path": "/sessions/payments/cvc",
        "headers": {
          "content-type": "application/vnd.worldpay.sessions-v1.hal+json",
          "Accept": "application/vnd.worldpay.sessions-v1.hal+json"
        },
        "body": {
          "cvc": "123",
          "identity": "incorrectValue"
        }
      },
      "response": {
        "status": 400,
        "headers": {
          "Content-Type": "application/vnd.worldpay.sessions-v1.hal+json"
        },
        "body": {
          "errorName": "bodyDoesNotMatchSchema",
          "validationErrors": [
            {
              "errorName": "fieldHasInvalidValue",
              "message": "Identity is invalid",
              "jsonPath": "$.identity"
            }
          ],
          "message": "The json body provided does not match the expected schema"
        },
        "matchingRules": {
          "$.body.validationErrors[0].message": {
            "match": "type"
          },
          "$.body.message": {
            "match": "type"
          }
        }
      }
    },
    {
      "description": "POST request to /sessions/card with valid body",
      "request": {
        "method": "post",
        "path": "/sessions/card",
        "headers": {
          "Accept": "application/vnd.worldpay.sessions-v1.hal+json",
          "content-type": "application/vnd.worldpay.sessions-v1.hal+json"
        },
        "body": {
          "identity": "identity",
          "cardExpiryDate": {
            "month": 12,
            "year": 2099
          },
          "cvc": "123",
          "cardNumber": "4111111111111111"
        }
      },
      "response": {
        "status": 201,
        "headers": {
          "Content-Type": "application/vnd.worldpay.sessions-v1.hal+json"
        },
        "body": {
          "_links": {
            "sessions:session": {
              "href": "http://pacttest/sessions/card/sampleSessionID"
            }
          }
        },
        "matchingRules": {
          "$.body._links.sessions:session.href": {
            "match": "regex",
            "regex": "https?:\\/\\/[^\\/]+\\/sessions\\/card\\/.+"
          }
        }
      }
    },
    {
      "description": "POST request to /sessions/payments/cvc with valid body",
      "request": {
        "method": "post",
        "path": "/sessions/payments/cvc",
        "headers": {
          "Accept": "application/vnd.worldpay.sessions-v1.hal+json",
          "content-type": "application/vnd.worldpay.sessions-v1.hal+json"
        },
        "body": {
          "cvc": "1234",
          "identity": "identity"
        }
      },
      "response": {
        "status": 201,
        "headers": {
          "Content-Type": "application/vnd.worldpay.sessions-v1.hal+json"
        },
        "body": {
          "_links": {
            "sessions:session": {
              "href": "http://pacttest/sessions/sessionURI"
            }
          }
        },
        "matchingRules": {
          "$.body._links.sessions:session.href": {
            "match": "regex",
            "regex": "https?:\\/\\/[^\\/]+\\/sessions\\/.+"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "3.0.0"
    }
  }
}